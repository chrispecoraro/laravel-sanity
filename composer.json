{
    "name": "eastslopestudio/laravel-sanity",
    "description": "A Sanity.io service provider for Laravel.",
    "license": "MIT",
    "authors": [{
      "name": "Ryan Scherler",
      "email": "ryan@eastslopestudio.com"
    }],
    "require": {
      "php": "^8.0.*",
      "sanity/sanity-php": "^1.1"
    },
    "require-dev": {
      "laravel/framework": "^8.0.*"
    },
    "autoload": {
      "psr-4": {
        "Eastslopestudio\\LaravelSanity\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Eastslopestudio\\LaravelSanity\\ServiceProvider"
        ],
        "aliases": {
          "Sanity": "Eastslopestudio\\LaravelSanity\\Facade"
        }
      }
    }
  }
